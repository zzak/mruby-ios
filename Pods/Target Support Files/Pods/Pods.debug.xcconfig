FRAMEWORK_SEARCH_PATHS = $(PODS_ROOT)/mruby-iOS
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/mruby-iOS"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/mruby-iOS"
OTHER_LDFLAGS = -ObjC -l"Pods-mruby-iOS" -framework "MRuby"
OTHER_LIBTOOLFLAGS = $(OTHER_LDFLAGS)
PODS_ROOT = ${SRCROOT}/Pods